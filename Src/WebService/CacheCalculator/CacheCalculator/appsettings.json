{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Debug",
      "Microsoft": "Debug"
    }
  },

    "Elasticsearch": {
        "Enabled": true,
        "Uri": "https://192.168.1.15:9200/",
        "IndexFormat": "dualogwebapi-{0:yyyy.MM.dd}",
        "MinimumLogEventLevel": 1
    },

  "Serilog": {
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "LiterateConsole"
      }
    ]
  }
}
